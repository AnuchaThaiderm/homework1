from flask import Flask
app = Flask(__name__)

@app.route('/')
def hello():
    return 'Hello World!'

if __name__ == '__main__':
    app.run(debug=True)

from flask import Flask
app = Flask(_name_)

@app.route('/')
def hello():
    return 'Hello anucha!'

@app.route('/Id')
def Id():
    return '654272114'

@app.route('/name')
def name():
    return 'Anucha thaiderm'

@app.route('/university')
def university():
    return 'Phetchaburi Rajabhat University'

if _name_ == '_main_':
    app.run(debug=True) 
